server {
    listen 80;
    listen [::]:80;
    server_name  _;
    location / {
        proxy_pass            http://localhost:{{ agama_port }};
    }
    location /prometheus {
        proxy_pass            http://localhost:9090;
    }
    location /grafana {
        proxy_set_header Host $http_host;
        proxy_pass            http://localhost:{{ grafana_port }};
    }
}